<?xml version="1.0" encoding="UTF-8" ?>
<parameters>
	
	<parameter name="number_of_offenders" displayName="Number of Offenders"
		type="int" defaultValue="10000" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />
		
	<parameter name="average_rate_of_offending" displayName="Rate of Offending"
		type="int" defaultValue="5" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />

	<parameter name="average_rate_of_reoffending" displayName="Rate of ReOffending"
		type="int" defaultValue="1" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />

	<parameter name="imprisonment_probability" displayName="Certainty Of Punishment (P(I)):"
		type="int" defaultValue="5" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />
				
	<parameter name="average_age" displayName="Average Population Age"
		type="int" defaultValue="70" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />
		
	<parameter name="age_standard_deviation" displayName="Standard Deviation of Population Age" type="int"
		defaultValue="10" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />
		
	<parameter name="average_time_in_prison" displayName="Average time in prison"
		type="int" defaultValue="5" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />
	
	<parameter name="standard_deviation_for_time_in_prison" displayName="Standard Deviation of Time in Prison"
		type="int" defaultValue="5" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />
		
	<parameter name="randomSeed" displayName="Default Random Seed"
		type="int" defaultValue="__NULL__" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />

</parameters>
